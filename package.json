{
  "name": "@goldenhippo/builder-workspace",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "packages/*",
    "@goldenhippo/*"
  ],
  "scripts": {
    "build": "nx run-many -t build",
    "build:builder-types": "nx run @goldenhippo/builder-types:build",
    "build:schemas": "nx run @goldenhippo/builder-cart-schemas:build",
    "build:funnel-schemas": "nx run @goldenhippo/builder-funnel-schemas:build",
    "build:plugin": "nx run @goldenhippo/builder-cart-plugin:build",
    "build:funnel-plugin": "nx run @goldenhippo/builder-funnel-plugin:build",
    "dev:plugin": "nx run @goldenhippo/builder-cart-plugin:dev",
    "dev:funnel-plugin": "nx run @goldenhippo/builder-funnel-plugin:dev",
    "lint": "nx run-many -t lint",
    "test": "nx run-many -t test",
    "typecheck": "nx run-many -t typecheck",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "changeset": "changeset",
    "version-packages": "changeset version",
    "release": "npm run build && changeset publish",
    "prepare": "husky"
  },
  "devDependencies": {
    "@changesets/changelog-github": "^0.5.2",
    "@changesets/cli": "^2.29.8",
    "@commitlint/cli": "^20.4.1",
    "@commitlint/config-conventional": "^20.4.1",
    "@nx/eslint-plugin": "^22.5.1",
    "@nx/js": "22.5.1",
    "eslint": "^9.39.2",
    "husky": "^9.1.7",
    "nx": "22.5.1",
    "prettier": "^3.8.1",
    "typescript": "^5.7.0",
    "typescript-eslint": "^8.55.0"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "packageManager": "npm@10.0.0"
}
