{
  "name": "@goldenhippo/builder-funnel-plugin",
  "version": "0.1.0",
  "description": "Builder.io plugin for Golden Hippo sales funnel websites",
  "main": "dist/plugin.system.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GoldenHippoMedia/golden-hippo-builder.git",
    "directory": "packages/builder-funnel-plugin"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "webpack-dev-server --mode development",
    "start": "webpack-dev-server --mode development",
    "build": "webpack --mode production",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src/"
  },
  "dependencies": {
    "@builder.io/app-context": "^1.0.0",
    "@goldenhippo/builder-cart-schemas": "*",
    "@goldenhippo/builder-ui": "*",
    "@tailwindcss/postcss": "^4.1.13",
    "clsx": "^2.1.1",
    "postcss": "^8.5.6",
    "react-cookie": "^8.0.1",
    "react-icons": "^5.5.0",
    "source-map-loader": "^5.0.0",
    "tailwindcss": "^4.1.13"
  },
  "devDependencies": {
    "@builder.io/react": "^9.1.0",
    "@tailwindcss/typography": "^0.5.19",
    "@types/react": "^18.3.28",
    "@types/react-dom": "^18.3.7",
    "css-loader": "^7.1.4",
    "daisyui": "^5.5.18",
    "mobx": "^6.15.0",
    "mobx-react": "^9.2.1",
    "postcss-loader": "^8.2.1",
    "postcss-prefix-selector": "^2.1.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "style-loader": "^4.0.0",
    "ts-loader": "^9.5.4",
    "webpack": "^5.105.2",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.3"
  },
  "nx": {
    "tags": [
      "scope:funnel",
      "type:plugin"
    ],
    "targets": {
      "build": {
        "outputs": [
          "{projectRoot}/dist"
        ],
        "dependsOn": [
          "^build"
        ]
      }
    },
    "implicitDependencies": [
      "@goldenhippo/builder-funnel-schemas",
      "@goldenhippo/builder-ui"
    ]
  }
}
